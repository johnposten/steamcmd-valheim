az container create \
     --cpu 2 \
     --memory 4 \
     --resource-group dev \
     --name seaman \
     --image johnposten/steamcmd-valheim \
     --dns-name-label seaman \
     --ports 2456 2457 2458 \
     --protocol UDP \
     --restart-policy Never \
     --environment-variables SERVER_NAME=whateverdude2 SERVER_PASSWORD=poop2 WORLD=whateverdude \
     --azure-file-volume-account-name seaman \
     --azure-file-volume-account-key 5MEzsY1D+8/N47r0Nh/NGI0eOlw7B/s3bqbV0c0FarQQW9yc9QYT+TzVkXpoQ1VH9GAy+s1Ww7a5oFUs4HTg4Q== \
     --azure-file-volume-share-name seamanshare \
     --azure-file-volume-mount-path /home/steam/


az container create \
     --cpu 2 \
     --memory 4 \
     --resource-group dev \
     --name seaman \
     --image lloesche/valheim-server \
     --dns-name-label seaman \
     --ports 2456 2457 2458 \
     --protocol UDP \
     --restart-policy Never \
     --environment-variables SERVER_NAME=whateverdude SERVER_PASS=poop1 WORLD_NAME=whateverdude \
     --azure-file-volume-account-name seaman \
     --azure-file-volume-account-key 5MEzsY1D+8/N47r0Nh/NGI0eOlw7B/s3bqbV0c0FarQQW9yc9QYT+TzVkXpoQ1VH9GAy+s1Ww7a5oFUs4HTg4Q== \
